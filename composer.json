{
    "name": "txthinking/util",
    "type": "library",
    "keywords": ["util"],
    "description": "Some util stuff",
    "license": "MIT",
    "homepage": "http://github.com/txthinking/util-php",
    "authors": [
        {
            "name": "Cloud",
            "email": "cloud@txthinking.com",
            "homepage": "http://www.txthinking.com",
            "role": "Thinker"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "redbeanphp",
                "version": "master",
                "dist": {
                    "url": "http://www.redbeanphp.com/downloadredbean.php",
                    "type": "tar"
                }
            }
        }

    ],
    "require": {
        "php": ">=5.3.2",
        "redbeanphp": "master",
        "vlucas/phpdotenv": "~1.1@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "files": [
            "bootstrap.php",
            "src/helper.php",
            "vendor/redbeanphp/rb.php"
        ],
        "psr-4": {
            "Tx\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "dev"
}
